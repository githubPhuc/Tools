<div class="row" style="margin-bottom:5px">
    <div class="col-md-12 col-sm-4">

        <div class="row" style="margin-bottom:5px">
            <label class="col-md-12 col-sm-12">Tài khoản:</label>
            <div class="col-md-12 col-sm-12 ten">
                <input value="" class="form-control" type="text" id="tenTaiKhoan" name="tenTaiKhoan" />
            </div>
        </div>
        <div class="row" style="margin-bottom:5px">
            <label class="col-md-12 col-sm-12">Mật khẩu:</label>
            <div class="col-md-12 col-sm-12 ten">
                <input value="" class="form-control" type="text" id="matKhau" name="matKhau" />
            </div>
        </div>
        <div class="row" style="margin-bottom:5px">
            <label class="col-md-12 col-sm-12">Tên nhân viên</label>
            <div class="col-md-12 col-sm-12 ten">
                <input value="" class="form-control" type="text" id="hoVaTen" name="hoVaTen" />
            </div>
        </div>
        <div class="row" style="margin-bottom:5px">
            <label class="col-md-12 col-sm-12">Email</label>
            <div class="col-md-12 col-sm-12 ten">
                <input oninput="this.value = this.value.trim()" value="" class="form-control" type="text" id="email" name="email" />
            </div>
        </div>
        <div class="row" style="margin-bottom:5px">
            <label class="col-md-12 col-sm-12">Số điện thoại</label>
            <div class="col-md-12 col-sm-12 ten">
                <input placeholder="Số điện thoại sẽ dùng làm tên tài khoản cho nhân viên" oninput="this.value = this.value.replace(/[^0-123456789]/g, '');" value="" class="form-control" type="text" id="soDienThoai" name="soDienThoai" />
            </div>
        </div>
        <div class="row" style="margin-bottom:5px">
            <label class="col-md-12 col-sm-12">dcThuongTru</label>
            <div class="col-md-12 col-sm-12 ten">
                <input  value="" class="form-control" type="text" id="dcThuongTru" name="dcThuongTru" />
            </div>
        </div>
        <div class="row" style="margin-bottom:5px">
            <label class="col-md-12 col-sm-12">dcTamTru</label>
            <div class="col-md-12 col-sm-12 ten">
                <input  value="" class="form-control" type="text" id="dcTamTru" name="dcTamTru" />
            </div>
        </div>
    </div>


</div>
<script type="text/javascript">

    var config = {
        '.chosen-select': {},
        '.chosen-select-deselect': { allow_single_deselect: true },
        '.chosen-select-no-single': { disable_search_threshold: 10 },
        '.chosen-select-no-results': { no_results_text: 'Oops, nothing found!' },
        '.chosen-select-width': { width: "125%" },
        'search_contains': true
    }
    for (var selector in config) {
        $(selector).chosen(config);
    }




</script>
