﻿
@{
    var user = ViewBag.user as ToolsApp.EntityFramework.User;
}

<input id="fontCCCD" value="@user.anhCCCDMatTruoc: " style="display:none" />
<input id="backCCCD" value="@user.anhCCCDMatSau:string" style="display:none" />
<main class="main_full">
    <div class="container">
        <div class="panel">
        </div>
        <div class="CCCD">
            <div class="uploaded_file_view" id="frontCCCD_View">
                <img class="show enlarge-image" data-fancybox="gallery"  src="@(!string.IsNullOrEmpty(user.anhCCCDMatTruoc) ? Url.Content(user.anhCCCDMatTruoc) : Url.Content("~/Content/images/noImage.png"))" alt="Mặt trước CCCD" />
                <strong>Mặt trước CCCD</strong>
            </div>
            <div class="uploaded_file_view" id="backCCCD_View">
                <img class="show enlarge-image" data-fancybox="gallery" src="@(!string.IsNullOrEmpty(user.anhCCCDMatSau) ? Url.Content(user.anhCCCDMatSau) : Url.Content("~/Content/images/noImage.png"))" alt="Mặt trước CCCD" />
                <strong>Mặt sau CCCD</strong>
            </div>
        </div>
    </div>

</main>
