
@{
    ViewBag.Title = "GetList";
    var data = ViewBag.KetCaus as List<ToolsApp.Models.KetCauModels>;
}
<table id="myTable" class="table table-bordered table-striped">
    <thead>
        <tr>
            <th></th>
            <th>Tên kết cấu</th>
            <th>Nguời tạo</th>
            <th>Ngày tạo</th>
            <th>Nguời cập nhật</th>
            <th>Ngày cập nhật</th>
            <th>Tình trạng</th>
        </tr>
    </thead>
    <tbody>
        @{ var STT = 1; }
        @{
            if (data != null)
            {
                foreach (var item in data)
                {
                    <tr>
                        <td style="max-width:40px!important">
                            <a onclick="Edit('@item.id')" class="btn">
                                <i class="fas fa-edit"></i> Edit
                            </a>
                            <a onclick="Delete('@item.id')" class="btn">
                                <i class="far fa-trash-alt"></i> Delete
                            </a>
                        </td>
                        <td>@item.tenKetCau</td>
                        <td>@item.tenNguoiTao</td>
                        <td>@item.ngayTao</td>
                        <td>@item.tenNguoiCapNhat</td>
                        <td>@item.ngayCapNhat</td>
                        <td>@item.trangThai</td>
                    </tr>

                    STT++;
                }
            }
           
        }

        </tbody>
    </table>
